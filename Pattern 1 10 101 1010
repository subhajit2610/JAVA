import java.util.Scanner;

public class pattern1 {
    public static void main(String[] args) {
        int no;
        System.out.println("Enter Your range: ");
        Scanner input = new Scanner(System.in);
        no = input.nextInt();
        for (int i = 1; i < no; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(j%2);
            }
            System.out.println("");
        }
    }
}
